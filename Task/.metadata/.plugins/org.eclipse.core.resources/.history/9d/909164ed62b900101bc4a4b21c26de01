package com.vehicle;

import java.util.Scanner;

public class Bike extends Vehicle{
	public	String CarRentalCost(int day)
		{
			int cost = 200 * day;
			System.out.println("The rental price for "+day +"is " +cost);
		}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("For how manys days do u want...");
		int day = sc.nextInt();
		Bike b = new Bike();
		b.CalculateRentalCost(day);
	}

}
