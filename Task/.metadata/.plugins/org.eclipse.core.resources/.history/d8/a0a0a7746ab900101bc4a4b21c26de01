import java.security.DomainCombiner;

public class Person {
	int age;
	int salary;
	String empname;
	Person()
	{
		System.out.println("Person constructor is running....");
	}
	void displayDetails(int age, int salary, String empname) {
		this.age = age;
        this.salary = salary;
        this.empname = empname;
		System.out.println("Name: "+this.empname);
		System.out.println("Age " +this.age);
		System.out.println("Salary " +this.salary);
	}
}
	class Employee extends Person
	{
		Employee()
		{
			super.displayDetails(22,35000,"Emp1");
			System.out.println("Employee constructor is running....");
		}
		void displayDetails(int age, int salary, String empname) {
			this.age = age;
	        this.salary = salary;
	        this.empname= empname;
			System.out.println("Name: "+this.empname);
			System.out.println("Age " +this.age);
			System.out.println("Salary "+this.salary);
		}
	}
	class Manager extends Employee
	{
		Manager()
		{
			super.displayDetails(22,30000,"Emp2");
			System.out.println("Manager constructor is running....");
		}
		void displayDetails(int age, int salary, String empname) {
			this.age = age;
	        this.salary = salary;
	        this.empname = empname;
			System.out.println("Name: "+this.empname);
			System.out.println("Age " +this.age);
			System.out.println("Salary "+this.salary);
		}
			public static void main(String[] args) {
			Manager m = new Manager();
			m.displayDetails(22, 33000, "Emp3");
	}
}