package outer;

public class Bank {
int amount=2000;
void balance()
{
	System.out.println("Account balace is " +amount);
}
void withdraw(int amount)
{
	amount -=this.amount;
}
void deposit(int amount)
{
	amount +=this.amount
	
}
	public static void main(String[] args) {
		
		Bank b1 = new Bank();
		b1.balance();
		b1.withdraw(500);
		b1.balance();
		//b1.deposit(1000);
		//b1.balance();
	}

}
