package com.demo;

public class Monkey extends Tiger implements Animal{
		public void eat()
		{ 
			//super.eat() prints only eats non-veg..!
			System.out.println("eats banana");
		}
		public static void main(String[] args) {
			Monkey m = new Monkey();
			m.eat();
			m.run();
			System.out.println("No of legs "+(legs+1));
		}
}
