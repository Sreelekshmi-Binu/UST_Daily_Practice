package pack;

public class Ascii {
	public static void main(String[] args)
	{
		String str1 = "WeLcOmE";
		int ascii = 0;
		String output="";
		for(int i=0;i<str1.length();i++)
		{
			ascii = (int)str1.charAt(i);
			if(ascii>=65 && ascii <=99)
			{
				output += String.valueOf((char)ascii).toLowerCase() ;
			}
			else
			{
				output += String.valueOf((char)ascii).toUpperCase() ;
			}
			}
		}
	}

}
