package pack;

public class Strdemo {
	public static void main(String[] args) {
		String str1 = "Welcome";
		String str5 ="*";
		String str2 = new String("Welcome");
		String str3 = new String("Welcome");
		System.out.println(str1==str2);
		System.out.println(str2==str3);
		System.out.println(str1.equals(str2));
		//str1 = str1.concat(str3);
				System.out.println(str1);
		System.out.println(str1.concat("to java")); 
		System.out.println(str1.charAt(3)); // to get a charaxter at a particular location
		System.out.println(str1.toUpperCase()); //to make all letters uppercase
		System.out.println(str1.length()); //to get lenght
		System.out.println(str1.replace('e', 'd')); //to replace a character
		System.out.println(str5.repeat(5));
		String str6 = "welcome to java";
		String arr[]= str6.split("");//to split an array
		for(int i =0; i<arr.length;i++)
		{
			System.out.println(arr[i]);
		}
	}

}
