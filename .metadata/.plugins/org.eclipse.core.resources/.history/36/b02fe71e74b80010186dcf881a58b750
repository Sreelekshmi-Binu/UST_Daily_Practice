package com.example;
class Ar
{
	Ar()
	{
		System.out.println("default Ar");
	}
}
class B extends Ar{
	B()
	{
		System.out.println("Default B");
	 }
}
class C extends B
{
	C()
	{
		System.out.println("Default C");
		
	}
}
public class Superr extends C {
	
public static void main(String[] args) {
	new Superr();
	
}
}
