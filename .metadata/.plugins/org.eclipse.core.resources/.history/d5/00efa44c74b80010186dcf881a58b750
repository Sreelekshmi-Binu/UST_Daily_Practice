package com.example;
class Ar
{
	Ar()
	{
		System.out.println("default Ar");
	}
	void A(int x)
	{
		System.out.println("va;ue of x is "+x);
	}
}
class Br extends Ar{
	Br()
	{
		super(5)
		System.out.println("Default Br");
	 }
}
class C extends Br
{
	C()
	{
		System.out.println("Default C");
		
	}
}
public class Superr extends C {
	
public static void main(String[] args) {
	new Superr();
	
}
}
