package com.example;
import java.util.Scanner;
public class prime {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("enter a number");
		int num = sc.nextInt();
				int count = 0;
				for(int i=2;i<num;i++)
				{
					if(num%i == 0)
					{
						count = count +1;
					}
				}
				if(count >1)
				{
					System.out.println("the no isn't prime..");
				}
				else
				{
					System.out.println("the no is prime..");
				}
	}

}
