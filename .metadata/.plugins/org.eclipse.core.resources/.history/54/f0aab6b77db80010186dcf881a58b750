package com.demo;

public class Tiger implements Animal {
	
@Override
public void run() {
	System.out.println("Tiger runs fast");	
}

@Override
public void eat() {
	System.out.println("Tiger eats non veg");
	
}
public static void main(String[] args)
{
	Tiger t =  new Tiger();
	t.eat();
	t.run();
	System.out.println("Tiger has " +(Animal.legs+1));
}
}
