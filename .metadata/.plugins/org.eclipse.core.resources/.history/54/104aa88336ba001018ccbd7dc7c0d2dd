package com.Int.ust.dao;

import java.util.ArrayList;

import javax.swing.JOptionPane;

import com.Int.ust.bean.DoctorBean;
import com.Int.ust.service.Administrator;

public class AdministratorDao implements Administrator {
ArrayList<DoctorBean> db ;
	@Override
	public String addDoctor(DoctorBean doctorbean) {
		db = new ArrayList<DoctorBean>();
		String id = JOptionPane.showInputDialog("UserID");
		String Fn = JOptionPane.showInputDialog("First Name");
		String Ln = JOptionPane.showInputDialog("Last Name");
		String DoB = JOptionPane.showInputDialog("Date Of Birth");
		String G = JOptionPane.showInputDialog("Gender");
		String st = JOptionPane.showInputDialog("Street");
        String loc = JOptionPane.showInputDialog("Location");
        String ci = JOptionPane.showInputDialog("City");
        String sta = JOptionPane.showInputDialog("State");
        String pin = JOptionPane.showInputDialog("PIN Code");
        String mob = JOptionPane.showInputDialog("Mobile Number");
        String mail = JOptionPane.showInputDialog("Email");
		db.add(new DoctorBean(id,Fn,Ln,DoB,G,st,loc,ci,sta,pin,mob,mail));
		
		return "SUCCESS";
		
	}

	@Override
	public ArrayList<DoctorBean> viewAllDoctor() {
		// TODO Auto-generated method stub
		return null;
	}

}
